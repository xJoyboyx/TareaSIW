extends ../partials/layout-components

//- Override default config variables
append config
  - var vendorStyles = ['prismjs/themes/prism.css', 'prismjs/plugins/toolbar/prism-toolbar.css', 'prismjs/plugins/line-numbers/prism-line-numbers.css']
  - var vendorScripts = ['prismjs/components/prism-core.min.js', 'prismjs/components/prism-markup.min.js', 'prismjs/components/prism-clike.min.js', 'prismjs/components/prism-javascript.min.js', 'prismjs/components/prism-pug.min.js', 'prismjs/plugins/toolbar/prism-toolbar.min.js', 'prismjs/plugins/copy-to-clipboard/prism-copy-to-clipboard.min.js', 'prismjs/plugins/line-numbers/prism-line-numbers.min.js']
  - var pageTitle = 'Countdown'
  - var pageSubtitle = 'Timer that counts down in seconds, minutes, hours and days to any date.'
  - var isPageTitleBadge = true
  - var anchorNav = [{'title': 'Basic example + Alignment', 'link': '#coundown-basic'}, {'title': 'Sizing', 'link': '#coundown-sizing'}, {'title': 'Colors', 'link': '#coundown-colors'}, {'title': 'Cards example', 'link': '#coundown-cards'}]
  
//- Page Content
block content
  .bg-secondary.rounded-lg.p-4.mb-5
    h6.font-size-sm Date / time format to use inside data attribute:
    code data-countdown='MM/DD/YYYY HH:mm:ss AM/PM'
    p.font-size-sm.mb-0.py-2: u Example:
    code data-countdown='10/01/2021 07:00:00 PM'

  // Basic example + Alignment
  section#coundown-basic.pb-5.mb-md-2
    h2.h4.mb-3 Basic example + Alignment
    .card.border-0.box-shadow-lg
      .card-header
        ul(role='tablist').nav.nav-tabs.card-header-tabs
          li.nav-item
            a(href='#result1' data-toggle='tab' role='tab' aria-controls='result1' aria-selected='true').nav-link.active Result
          li.nav-item
            a(href='#html1' data-toggle='tab' role='tab' aria-controls='html1' aria-selected='false').nav-link HTML
          li.nav-item
            a(href='#pug1' data-toggle='tab' role='tab' aria-controls='pug1' aria-selected='false').nav-link Pug
      .card-body
        .tab-content
          #result1.tab-pane.fade.show.active(role='tabpanel')
            .cs-countdown.py-2.h4(data-countdown='10/01/2021 07:00:00 PM')
              .cs-countdown-days
                span.cs-countdown-value 0
                span.cs-countdown-label.text-body Days
              .cs-countdown-hours
                span.cs-countdown-value 0
                span.cs-countdown-label.text-body Hours
              .cs-countdown-minutes
                span.cs-countdown-value 0
                span.cs-countdown-label.text-body Mins
              .cs-countdown-seconds
                span.cs-countdown-value 0
                span.cs-countdown-label.text-body Secs
            .cs-countdown.justify-content-center.py-2.h4(data-countdown='10/01/2021 07:00:00 PM')
              .cs-countdown-days
                span.cs-countdown-value 0
                span.cs-countdown-label.text-body Days
              .cs-countdown-hours
                span.cs-countdown-value 0
                span.cs-countdown-label.text-body Hours
              .cs-countdown-minutes
                span.cs-countdown-value 0
                span.cs-countdown-label.text-body Mins
              .cs-countdown-seconds
                span.cs-countdown-value 0
                span.cs-countdown-label.text-body Secs
            .cs-countdown.justify-content-end.py-2.h4(data-countdown='10/01/2021 07:00:00 PM')
              .cs-countdown-days
                span.cs-countdown-value 0
                span.cs-countdown-label.text-body Days
              .cs-countdown-hours
                span.cs-countdown-value 0
                span.cs-countdown-label.text-body Hours
              .cs-countdown-minutes
                span.cs-countdown-value 0
                span.cs-countdown-label.text-body Mins
              .cs-countdown-seconds
                span.cs-countdown-value 0
                span.cs-countdown-label.text-body Secs
          #html1.tab-pane.fade(role='tabpanel')
            pre.line-numbers
              code(class='lang-html').
                &lt;!-- Left aligned countdown (default) --&gt;
                &lt;div class=&quot;cs-countdown h4&quot; data-countdown=&quot;10/01/2021 07:00:00 PM&quot;&gt;
                  &lt;div class=&quot;cs-countdown-days&quot;&gt;
                    &lt;span class=&quot;cs-countdown-value&quot;&gt;0&lt;/span&gt;
                    &lt;span class=&quot;cs-countdown-label text-body&quot;&gt;Days&lt;/span&gt;
                  &lt;/div&gt;
                  &lt;div class=&quot;cs-countdown-hours&quot;&gt;
                    &lt;span class=&quot;cs-countdown-value&quot;&gt;0&lt;/span&gt;
                    &lt;span class=&quot;cs-countdown-label text-body&quot;&gt;Hours&lt;/span&gt;
                  &lt;/div&gt;
                  &lt;div class=&quot;cs-countdown-minutes&quot;&gt;
                    &lt;span class=&quot;cs-countdown-value&quot;&gt;0&lt;/span&gt;
                    &lt;span class=&quot;cs-countdown-label text-body&quot;&gt;Mins&lt;/span&gt;
                  &lt;/div&gt;
                  &lt;div class=&quot;cs-countdown-seconds&quot;&gt;
