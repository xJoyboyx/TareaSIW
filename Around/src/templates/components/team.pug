  // Style 1
  section#team-style-1.pb-5.mb-md-2
    h2.h4.mb-3 Style 1
    .card.border-0.box-shadow-lg
      .card-header
        ul(role='tablist').nav.nav-tabs.card-header-tabs
          li.nav-item
            a(href='#result1' data-toggle='tab' role='tab' aria-controls='result1' aria-selected='true').nav-link.active Result
          li.nav-item
            a(href='#html1' data-toggle='tab' role='tab' aria-controls='html1' aria-selected='false').nav-link HTML
          li.nav-item
            a(href='#pug1' data-toggle='tab' role='tab' aria-controls='pug1' aria-selected='false').nav-link Pug
      .card-body
        .tab-content
          #result1.tab-pane.fade.show.active(role='tabpanel')
            .d-sm-flex
              a(href='#' style='max-width: 18rem;').card.card-curved-body.card-hover.mr-4.mb-4
                .card-floating-icon: i.fe-instagram
                .card-img-top.card-img-gradient
                  +image('team/01.jpg', 'Sarah Cole')
                .card-body.text-center
                  h3.h6.card-title.mb-2 Sarah Cole
                  p.font-size-xs.text-body.mb-0 Social media strategist
              .card.card-curved-body.card-hover.border-0.box-shadow.mb-4(style='max-width: 18rem;')
                a(href='#').card-floating-icon: i.fe-mail
                .card-img-top.card-img-gradient
                  +image('team/02.jpg', 'Charlie Welch')
                .card-body.text-center
                  h3.h6.card-title.mb-2 Charlie Welch
                  p.font-size-xs.text-body.mb-0 Financial director
          #html1.tab-pane.fade(role='tabpanel')
            pre.line-numbers
              code(class='lang-html').
                &lt;!-- Team Style 1: Border + whole card is a link --&gt;
                &lt;a class=&quot;card card-curved-body card-hover&quot; href=&quot;#&quot;&gt;
                  &lt;div class=&quot;card-floating-icon&quot;&gt;
                    &lt;i class=&quot;fe-instagram&quot;&gt;&lt;/i&gt;
                  &lt;/div&gt;
                  &lt;div class=&quot;card-img-top card-img-gradient&quot;&gt;
                    &lt;img src=&quot;path-to-image&quot; alt=&quot;Sarah Cole&quot;/&gt;
