extends partials/layout

//- Override default config variables
append config
  - var vendorStyles = ['tiny-slider/dist/tiny-slider.css']
  - var vendorScripts = ['tiny-slider/dist/min/tiny-slider.js', 'jarallax/dist/jarallax.min.js', 'imagesloaded/imagesloaded.pkgd.min.js', 'shufflejs/dist/shuffle.min.js']
  - var parentMenu = 'Demos'
  - var pageTitle = 'Web Studio'

//- Navbar
block navbar
  include partials/navbar-floating-light

//- Page Content
block content

  // Hero
  section.jarallax.bg-dark.pt-5.pt-lg-7.pb-7(data-jarallax data-speed='0.3')
    .jarallax-img(style='background-image: url(img/demo/web-studio/cubes-bg.jpg);')
    .cs-shape.cs-shape-bottom.cs-shape-curve.bg-body
      svg(xmlns='http://www.w3.org/2000/svg' viewBox='0 0 3000 185.4')
        path(fill='currentColor' d='M3000,0v185.4H0V0c496.4,115.6,996.4,173.4,1500,173.4S2503.6,115.6,3000,0z')
    
    // Text
    .container.bg-overlay-content.text-center.pt-6.pt-lg-7
      h2.font-size-xl.font-weight-normal.text-light Award winning web design agency
      h1.text-light Get your website done

    .d-block.d-sm-none(style='height: 100px;')
    .d-none.d-sm-block.d-lg-none(style='height: 300px;')
    .d-none.d-lg-block(style='height: 400px;')


  // Featured Projects (carousel)
  section.container.bg-overlay-content
    .d-block.d-sm-none(style='margin-top: -160px;')
    .d-none.d-sm-block.d-lg-none(style='margin-top: -360px;')
    .d-none.d-lg-block(style='margin-top: -450px;')
    .cs-frame-browser.border-light.cs-carousel.mx-auto(style='max-width: 915px;')
      .cs-frame-browser-toolbar
        .text-nowrap.mr-3
          span.cs-frame-browser-button.bg-danger
          span.cs-frame-browser-button.bg-warning
          span.cs-frame-browser-button.bg-success
        span.cs-carousel-label.font-size-sm.text-light.opacity-75 https://mobileapp.com
      .cs-frame-browser-body
        .cs-carousel-inner(data-carousel-options='{"mode": "gallery", "controls": false, "nav": false, "autoplay": true, "autoplayTimeout": 6000}')
          div(data-carousel-label='https://mobileapp.com')
            +image('demo/web-studio/slider/01.jpg', 'Screenshot').rounded
          div(data-carousel-label='https://bookingapp.com')
            +image('demo/web-studio/slider/02.jpg', 'Screenshot').rounded
          div(data-carousel-label='https://creativeagency.com')
            +image('demo/web-studio/slider/03.jpg', 'Screenshot').rounded
      .cs-carousel-pager.pt-4.text-nowrap.text-center.mt-2.mb-n2
